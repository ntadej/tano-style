{
  "name": "@ntadej/style",
  "version": "0.4.0",
  "description": "Common infrastructure for tano.si webpages",
  "license": "MIT",
  "author": "Tadej Novak <tadej@tano.si>",
  "homepage": "https://github.com/ntadej/tano-style#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntadej/tano-style.git"
  },
  "scripts": {
    "lint-eslint": "next lint",
    "lint-eslint-fix": "next lint --fix --max-warnings=0",
    "lint-typescript": "tsc --noEmit --declaration",
    "prettier-check": "prettier --check . --loglevel warn",
    "prettier-fix": "prettier --write .",
    "lint-fix": "run-s prettier-fix lint-eslint-fix",
    "lint": "run-p lint-typescript prettier-check lint-eslint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@fontsource/inter": "^4.5.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@icons-pack/react-simple-icons": "^7.2.0",
    "next": "~13.4.0",
    "next-themes": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "~13.4.0",
    "npm-run-all": "~4.1.0",
    "postcss": "^8.4.0",
    "pre-commit": "~1.2.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "~0.3.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
